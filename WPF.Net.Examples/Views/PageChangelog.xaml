<Page x:Class="WPF.Net.Examples.Views.PageChangelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        DataContext="{StaticResource AppSettings}"
        Title="WebClient"
        Width="780" Height="600" 
        Loaded="PageChangelog_Loaded">

    <Grid Name="GridChangeLog">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <ScrollViewer>
            <TextBlock Name="TextBlockMain" Margin="5" Foreground="Black" FontSize="15" ></TextBlock>
        </ScrollViewer>
    </Grid>
</Page>
